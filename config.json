{
  "name": "redis-$RANDOM_STRING_5$",
  "namespace": "default",
  "run":[
    {
      "name": "engine-ecks",
      "type": "deployment",
      "config_file": "deploy.json",
      "containers": [
        {
          "env": [
            {
              "name": "LOGIN",
              "value": "$RANDOM_STRING_5$",
              "desc": "description 1",
            },
            {
              "name": "PASSWORD",
              "value": "$RANDOM_STRING_5$",
              "desc": "description password"
            }
          ],
          "name": "engine-ecks-cont"
        }
      ]
    },
    {
      "type": "service",
      "config_file": "service.json",
      "name": "engine-ecks-svc",
      "deployment": "engine-ecks"
    }
  ]
}